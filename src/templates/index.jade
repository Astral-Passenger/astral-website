        section#contact
          .container
            .row
              .col-lg-8.col-lg-offset-2.text-center
                h2.section-heading WORK WITH US
                |                     
                hr.primary
                |                     
                p
                  | Need a website or app built? Give us a call, or fill out our contact form below to get started on your projecet today. We would love to work with you!
              |                 
              .col-lg-4.col-lg-offset-2.text-center
                i.fa.fa-phone.fa-3x.wow.bounceIn
                |                     
                p.contact-info 559-940-9699
              |                 
              .col-lg-4.text-center
                i.fa.fa-envelope-o.fa-3x.wow.bounceIn(data-wow-delay='.1s')
                |                     
                p.contact-info
                  button.btn.btn-primary.btn-lg(type='button', data-toggle='modal', data-target='#myModal') GET IN TOUCH!
              .social-icons
                a(href='http://www.facebook.com').col-sm-4.social-icon.facebook
                  i.fa.fa-facebook 
                a(href='http://www.twitter.com').col-sm-4.social-icon.twitter
                  i.fa.fa-twitter
                a(href='http://www.instagram.com').col-sm-4.social-icon.instagram
                  i.fa.fa-instagram
          // Modal
          #myModal.modal.fade(tabindex='-1', role='dialog', aria-labelledby='myModalLabel')
            .modal-dialog(role='document')
              .modal-content
                .modal-header
                  button.close(type='button', data-dismiss='modal', aria-label='Close')
                    span(aria-hidden='true') ×
                  h4#myModalLabel.modal-title WORK WITH US
                .modal-body
                  form.contact-form(action='/', method='post', novalidate='novalidate')
                    div(style='display: none;')
                      input(type='hidden', name='_wpcf7', value='482')
                      input(type='hidden', name='_wpcf7_version', value='4.3.1')
                      input(type='hidden', name='_wpcf7_locale', value='en_US')
                      input(type='hidden', name='_wpcf7_unit_tag', value='wpcf7-f482-o1')
                      input(type='hidden', name='_wpnonce', value='23048474a7')
                    .row
                      div
                        .row
                          .col-md-6
                            span.wpcf7-form-control-wrap.FirstName
                              input.wpcf7-form-control.wpcf7-text.wpcf7-validates-as-required.form-control.username-input(type='text', name='FirstName', size='40', aria-required='true', aria-invalid='false', placeholder='First', data-bind='textInput: userName, event: {keypress: enableDetails}')
                          .col-md-6
                            span.wpcf7-form-control-wrap.LastName
                              input.wpcf7-form-control.wpcf7-text.wpcf7-validates-as-required.form-control(type='text', name='LastName', value='', size='40', aria-required='true', aria-invalid='false', placeholder='Last')
                        .row
                          .col-md-12
                            span.wpcf7-form-control-wrap.companyname
                              input#footer-company-blue-background.wpcf7-form-control.wpcf7-text.wpcf7-validates-as-required.form-control(type='text', name='companyname', value='', size='40', aria-required='true', aria-invalid='false', placeholder='Company Name')
                        .row
                          .col-md-12
                            span.wpcf7-form-control-wrap.email
                              input#footer-contact-email-blue-background.wpcf7-form-control.wpcf7-text.wpcf7-email.wpcf7-validates-as-required.wpcf7-validates-as-email.form-control(type='email', name='email', value='', size='40', aria-required='true', aria-invalid='false', placeholder='Email')
                          .col-md-12
                            span
                              textarea(rows='4')(name='message', placeholder='Enter message here...')
                    p
                      input#footer-submit-blue-background.btn.btn-primary.wpcf7-form-control.wpcf7-submit(type='submit', value='SUBMIT')
                    .wpcf7-response-output.wpcf7-display-none


        section#footer
          .container
